{
  "version": 2,
  "projects": {
    "wallet": {
      "root": "apps/wallet",
      "sourceRoot": "apps/wallet/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/wallet",
            "index": "apps/wallet/src/index.html",
            "baseHref": "/",
            "main": "apps/wallet/src/main.tsx",
            "polyfills": "apps/wallet/src/polyfills.ts",
            "tsConfig": "apps/wallet/tsconfig.app.json",
            "assets": ["apps/wallet/src/favicon.ico", "apps/wallet/src/assets"],
            "styles": ["apps/wallet/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/wallet/src/environments/environment.ts",
                  "with": "apps/wallet/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "wallet:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "wallet:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/wallet/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/wallet"],
          "options": {
            "jestConfig": "apps/wallet/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "wallet-e2e": {
      "root": "apps/wallet-e2e",
      "sourceRoot": "apps/wallet-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/wallet-e2e/cypress.json",
            "devServerTarget": "wallet:serve",
            "tsConfig": "apps/wallet-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wallet:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/wallet-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["wallet"]
    }
  }
}
